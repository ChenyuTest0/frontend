{
  "name": "mcoe-vuejs-assetize",
  "version": "1.8.2",
  "scripts": {
    "start": "npm-run-all -p \"dev:* --{1} {2}\" --",
    "dev:server": "vite",
    "dev:mock": "cd ./mock_server && pnpm i && pnpm start",
    "build": "cross-env NODE_ENV=development vite build",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "serve": "vite preview",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint \"src/**/*.ts\" \"src/**/*.vue\"",
    "lint:styles": "stylelint \"src/**/*.scss\" \"src/**/*.html\" \"src/**/*.vue\" --config .stylelintrc.json",
    "lint:type-check": "vue-tsc --noEmit --skipLibCheck -p tsconfig.json --composite false",
    "audit": "mkdirp ./reports/ && pnpm dlx auditjs ossi -d --xml > ./reports/scan_node_modules.xml",
    "test": "vitest watch src --ui --open",
    "e2e": "playwright test",
    "test:ci": "vitest run src --coverage",
    "test:e2e": "start-server-and-test start http-get://localhost:3000 e2e",
    "test:e2e:ci": "cross-env CI=true start-server-and-test start http-get://localhost:3000 e2e",
    "dts-gen": "pnpm dlx dts-gen",
    "openapi-gen": "pnpm --package=openapi-typescript-codegen dlx openapi",
    "clean": "pnpm store prune & rm -rf .pnpm-store node_modules",
    "license-checker": "pnpm dlx license-checker --csv > licenses.csv",
    "prepare": "husky",
    "ci": "pnpm i --frozen-lockfile",
    "update": "pnpx npm-check-updates -u"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/postcss": "4.1.7",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "@vuepic/vue-datepicker": "9.0.3",
    "@vueuse/core": "11.1.0",
    "@vueuse/integrations": "11.1.0",
    "animejs": "3.2.2",
    "axios": "1.8.3",
    "budoux": "0.6.2",
    "chart.js": "4.4.4",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "deepmerge": "4.3.1",
    "focus-trap": "7.6.0",
    "jenesius-vue-modal": "1.11.9",
    "pinia": "2.2.7",
    "pinia-plugin-persistedstate": "4.1.3",
    "sanitize-html": "2.13.1",
    "stacktrace-js": "2.0.2",
    "tailwindcss": "4.1.7",
    "ts-custom-error": "3.3.1",
    "tsyringe": "4.8.0",
    "vee-validate": "4.13.2",
    "vue": "3.5.13",
    "vue-chartjs": "5.3.1",
    "vue-i18n": "11.1.2",
    "vue-router": "4.4.5",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "4.0.0",
    "@pinia/testing": "0.1.6",
    "@playwright/test": "1.47.2",
    "@rollup/plugin-strip": "3.0.4",
    "@tailwindcss/vite": "4.1.7",
    "@types/animejs": "3.1.12",
    "@types/node": "20.14.2",
    "@types/sanitize-html": "2.13.0",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitest/coverage-istanbul": "3.0.8",
    "@vitest/ui": "3.0.8",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "@vue/test-utils": "2.4.6",
    "axios-mock-adapter": "2.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-vue": "9.22.0",
    "husky": "9.1.6",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.10",
    "mkdirp": "3.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.47",
    "postcss-html": "1.7.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "reflect-metadata": "0.2.2",
    "rollup": "4.23.0",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.79.4",
    "start-server-and-test": "2.0.8",
    "stylelint": "16.19.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended-scss": "14.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.0",
    "stylelint-junit-formatter": "0.2.2",
    "stylelint-scss": "6.3.1",
    "typescript": "5.6.2",
    "typescript-eslint": "8.8.0",
    "vite": "6.2.1",
    "vitest": "3.0.8",
    "vue-eslint-parser": "9.4.3",
    "vue-router-mock": "1.1.0",
    "vue-tsc": "2.1.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint",
      "prettier --check"
    ],
    "src/**/*.{html,vue,scss}": "stylelint"
  },
  "pnpm": {
    "overrides": {
      "word-wrap@<1.2.4": ">=1.2.4",
      "semver@<5.7.2": ">=5.7.2",
      "stylelint-config-recommended@<14.0.0": ">=14.0.0",
      "@intlify/shared@>=9.7.0 <9.14.2": ">=9.14.2",
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  },
  "engines": {
    "node": "20.11.1"
  }
}
